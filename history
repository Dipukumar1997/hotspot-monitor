  613  sudo tee /usr/share/gnome-shell/extensions/hotspot-indicator@hotspot/extension-main.js >/dev/null <<'EOF'
  614  const ByteArray = imports.byteArray;
  615  const { St, GLib, Gio, Clutter } = imports.gi;
  616  const Main = imports.ui.main;
  617  const PanelMenu = imports.ui.panelMenu;
  618  const STATE_FILE = GLib.get_home_dir() + '/.local/share/hotspot-usage/state.json';
  619  function human(n) {
  620      let step = 1024.0;
  621      let units = ['B', 'KB', 'MB', 'GB', 'TB'];
  622      let v = n;
  623      for (let i = 0; i < units.length; i++) {
  624          if (v < step)
  625              return v.toFixed(2) + ' ' + units[i];
  626          v = v / step;
  627      }
  628      return v.toFixed(2) + ' PB';
  629  }
  630  var HotspotIndicator = class extends PanelMenu.Button {
  631      _init() {
  632          PanelMenu.Button.prototype._init.call(this, 0.0, 'Hotspot Indicator');
  633          this._label = new St.Label({
  634              text: '…',
  635              y_align: Clutter.ActorAlign.CENTER,
  636          });
  637          this.add_child(this._label);
  638          this.connect('button-press-event', () => {
  639              GLib.spawn_command_line_async('/usr/local/bin/hotspot-gui');
  640          });
  641          this._timeout = GLib.timeout_add_seconds(
  642              GLib.PRIORITY_DEFAULT,
  643              1,
  644              () => {
  645                  this._refresh();
  646                  return true;
  647              }
  648          );
  649      }
  650      _refresh() {
  651          try {
  652              let [ok, contents] = GLib.file_get_contents(STATE_FILE);
  653              if (!ok)
  654                  return;
  655              let txt = ByteArray.toString(contents);
  656              let obj = JSON.parse(txt);
  657              let today = obj.today || {};
  658              let down = obj.speed_down_bps || 0;
  659              let totalToday = (today.bytes_recv || 0) + (today.bytes_sent || 0);
  660              let downH = human(down) + '/s';
  661              let totalH = human(totalToday);
  662              this._label.text = '↓ ' + downH + '  |  Today ' + totalH;
  663          } catch (e) {
  664              this._label.text = '↓ - | Today -';
  665          }
  666      }
  667      destroy() {
  668          if (this._timeout) {
  669              GLib.source_remove(this._timeout);
  670              this._timeout = 0;
  671          }
  672          PanelMenu.Button.prototype.destroy.call(this);
  673      }
  674  };
  675  let indicator = null;
  676  function init() {
  677  }
  678  function enable() {
  679      if (!indicator) {
  680          indicator = new HotspotIndicator();
  681          Main.panel.addToStatusArea('hotspot-indicator', indicator, 1, 'right');
  682      }
  683  }
  684  function disable() {
  685      if (indicator) {
  686          indicator.destroy();
  687          indicator = null;
  688      }
  689  }
  690  EOF
