  728  cat > indicator.js <<'EOF'
  729  import St from 'gi://St';
  730  import GLib from 'gi://GLib';
  731  import Clutter from 'gi://Clutter';
  732  import * as PanelMenu from 'resource:///org/gnome/shell/ui/panelMenu.js';
  733  import * as Main from 'resource:///org/gnome/shell/ui/main.js';
  734  const STATE_FILE = GLib.get_home_dir() + '/.local/share/hotspot-usage/state.json';
  735  function human(n) {
  736      let step = 1024.0;
  737      let units = ['B', 'KB', 'MB', 'GB', 'TB'];
  738      let v = n;
  739      for (let i = 0; i < units.length; i++) {
  740          if (v < step)
  741              return `${v.toFixed(2)} ${units[i]}`;
  742          v = v / step;
  743      }
  744      return `${v.toFixed(2)} PB`;
  745  }
  746  export class HotspotIndicator extends PanelMenu.Button {
  747      constructor() {
  748          super(0.0, 'Hotspot Indicator');
  749          this._label = new St.Label({
  750              text: '…',
  751              y_align: Clutter.ActorAlign.CENTER,
  752          });
  753          this.add_child(this._label);
  754          this.connect('button-press-event', () => {
  755              GLib.spawn_command_line_async('/usr/local/bin/hotspot-gui');
  756          });
  757          this._timeoutId = GLib.timeout_add_seconds(
  758              GLib.PRIORITY_DEFAULT,
  759              1,
  760              () => {
  761                  this._refresh();
  762                  return GLib.SOURCE_CONTINUE;
  763              }
  764          );
  765          // initial refresh
  766          this._refresh();
  767      }
  768      _refresh() {
  769          try {
  770              let [ok, contents] = GLib.file_get_contents(STATE_FILE);
  771              if (!ok)
  772                  return;
  773              // contents is a Uint8Array in module mode
  774              let txt = new TextDecoder().decode(contents);
  775              let obj = JSON.parse(txt);
  776              let today = obj.today || {};
  777              let down = obj.speed_down_bps || 0;
  778              let totalToday = (today.bytes_recv || 0) + (today.bytes_sent || 0);
  779              let downH = human(down) + '/s';
  780              let totalH = human(totalToday);
  781              this._label.text = `↓ ${downH}  |  Today ${totalH}`;
  782          } catch (e) {
  783              this._label.text = '↓ - | Today -';
  784          }
  785      }
  786      destroy() {
  787          if (this._timeoutId) {
  788              GLib.source_remove(this._timeoutId);
  789              this._timeoutId = 0;
  790          }
  791          super.destroy();
  792      }
  793  }
  794  EOF
  795  cat > extension.js <<'EOF'
  796  import { Extension } from 'resource:///org/gnome/shell/extensions/extension.js';
  797  import * as Main from 'resource:///org/gnome/shell/ui/main.js';
  798  import { HotspotIndicator } from './indicator.js';
  799  export default class HotspotIndicatorExtension extends Extension {
  800      enable() {
  801          this._indicator = new HotspotIndicator();
  802          Main.panel.addToStatusArea('hotspot-indicator', this._indicator, 1, 'right');
  803      }
  804      disable() {
  805          if (this._indicator) {
  806              this._indicator.destroy();
  807              this._indicator = null;
  808          }
  809      }
  810  }
  811  EOF
